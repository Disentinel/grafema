{
  "name": "@grafema/rfdb-client",
  "version": "0.2.7",
  "description": "TypeScript client for RFDB (Rega Flow Database)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js"
    }
  },
  "files": [
    "dist",
    "ts"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "grafema",
    "graph",
    "database",
    "code-analysis",
    "rust",
    "rega-flow",
    "rfdb"
  ],
  "license": "Apache-2.0",
  "author": "Vadim Reshetnikov",
  "repository": {
    "type": "git",
    "url": "https://github.com/Disentinel/grafema.git",
    "directory": "packages/rfdb"
  },
  "dependencies": {
    "@grafema/types": "workspace:*",
    "@msgpack/msgpack": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 18"
  }
}
