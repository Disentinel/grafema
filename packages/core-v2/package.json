    "clean": "rm -rf dist"
  },
  "license": "Apache-2.0",
  "author": "Vadim Reshetnikov",
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/types": "^7.26.3",
    "@grafema/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "typescript": "^5.9.3"
  }
}
